{
  "name": "SoulKitten",
  "version": "07.01.2025",
  "private": true,
  "description": "simple discord bot",
  "main": "build/main.js",
  "author": "sebastianjnuwu <sebastianjnuwu@gmail.com>",
  "license": "Apache-2.0",
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "build:watch": "tsc --project tsconfig.json -w",
    "start": "node build/main.js",
    "dev": "tsnd --expose-gc --transpile-only --ignore-watch node_modules --respawn src/main.ts",
    "lint": "npx @biomejs/biome lint --write",
    "format": "npx @biomejs/biome format --write",
    "check": "tsc --noEmit",
    "prepare": "husky",
    "pre-commit": "cz"
  },
  "hooks": {
    "prepare-commit-msg": "npx --no -- commitlint --edit $1; exec < /dev/tty && npx cz --hook || true"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@prisma/client": "^6.0.0",
    "child_process": "^1.0.2",
    "colors": "^1.4.0",
    "discord.js": "^14.14.1",
    "i18next": "^24.0.0",
    "i18next-fs-backend": "^2.3.1",
    "ms": "^2.1.3",
    "os": "^0.1.2",
    "prisma": "^6.0.0",
    "undici": "^7.0.0",
    "util": "^0.12.5"
  },
  "devDependencies": {
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^9.1.7",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.0.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sebastianjnuwu/SoulKitten"
  },
  "bugs": {
    "url": "https://github.com/sebastianjnuwu/SoulKitten/issues"
  },
  "engines": {
    "npm": ">=8",
    "node": ">=18"
  }
}
